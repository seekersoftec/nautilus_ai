    "simulate_model": {
        "data_start": 0,
        "data_end": null,
        "direction": "long",
        "signal_generator": "threshold_rule", 
        "buy_sell_equal": false,
        "grid": {
            "buy_signal_threshold": [0.02, 0.03, 0.04, 0.05, 0.1, 0.15],
            "sell_signal_threshold": [-0.02, -0.03, -0.04, -0.05, -0.1, -0.15]
        }
    },
    "rolling_predict": {
